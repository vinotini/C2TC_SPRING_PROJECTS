<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
           http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="jio" class="ioc.Jio"/>
    <bean id="oppo" class="ioc.Oppo"/>
    <bean id="samsung" class="ioc.Samsung"/>

    <bean id="diStudent1" class="dependencyInjection.Student">
        <property name="name" value="Di Vinti"/>
    </bean>

    <bean id="diStudent2" class="dependencyInjection.Student1">
        <property name="name" value="Di Vinotini"/>
    </bean>

</beans>
